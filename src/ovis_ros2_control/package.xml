<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ovis_ros2_control</name>
  <version>0.3.0</version>
  <description>
     An automatically generated package with all the configuration and launch files for using the ovis with the MoveIt Motion Planning Framework
  </description>
  <maintainer email="capra@ens.etsmtl.ca">capra</maintainer>

  <license>MIT</license>

  <author email="capra@ens.etsmtl.ca">capra</author>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>hardware_interface</depend>

  <depend>pluginlib</depend>

  <depend>rclcpp</depend>

  <depend>rclcpp_lifecycle</depend>

  <exec_depend>ovis_description</exec_depend>
  <exec_depend>joint_state_publisher</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>tf2_ros</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
